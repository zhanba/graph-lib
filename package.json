{
  "name": "graph-lib",
  "version": "0.1.0",
  "description": "A graph lib based on graphlib and write in typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "lint": "",
    "test": "jest"
  },
  "author": "ryannz",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "jest": "^22.0.6",
    "ts-jest": "^22.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.91",
    "lodash": "^4.17.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
